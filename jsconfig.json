{
  "compilerOptions": {
    "sourceMap": true,
    "module": "ES6",
    "target": "ESNext",
    "allowJs": true,
    "checkJs": false,
    "baseUrl": "./",
    "paths": {
      "SPLINT": [
        "../Splint/js/modules/ThreeJS/CORE.js"
      ],
      "splint": [
        "../Splint/js/modules/ThreeJS/CORE.js"
      ],
    //   "three": [
    //     "https://unpkg.com/three@v0.162.0/build/three.module.js"
    //   ],
      "@THREE_ADDONS/*": [
        "../Splint/node_modules/three/examples/jsm/*"
      ],
      "@THREE_SRC/*": [
        "../Splint/node_modules/three/src/*"
      ],
      "@THREE": [
        "../Splint/node_modules/three/build/three.module.js"///threeJS/build/three.module.js"
      ],
      "three/*": [
        "../Splint/node_modules/three/build/three.module.js"///threeJS/build/three.module.js"
      ],
      "threeJS": [
        "../Splint/lib/threeJS/build/three.module.js"
      ],
      "@THREE_ROOT_DIR/*": [
        "../Splint/lib/threeJS/*"
      ],
      "@THREE_MODULES_DIR/*": [
        "../Splint/lib/threeJS/examples/jsm/*"
      ],
      "@SPLINT_MODULES_DIR/*": [
        "../Splint/js/modules/*"
      ],
      "@SPLINT_THREE_DIR/*": [
        "../Splint/js/modules/ThreeJS/*"
      ]
    }
  },
  "include": ["resources/js/**/*", "../Splint/js/**/*", "../Splint/lib/imageTracerJS/**/*"]
}